{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "userChats": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "chats": {
      "$chatId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "users": {
      "$uid": {
        ".read": "auth != null",
        ".write": "$uid === auth.uid"
      }
    },
    "test": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}